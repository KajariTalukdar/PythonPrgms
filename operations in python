num1 = input("enter number 1: ")
num2 = input("Enter number 2: ")
result_sum = int (num1)+ int(num2)
result_substract = int (num1) - int(num2)
result_div = int (num1) / int(num2)
result_exp = int (num1)**int(num2)
result_multiply = int(num1)* int(num2)
print ("sum: ", result_sum)
print ("substract: ",result_substract)
print ("division: ",result_div)
print ("Multiply: ",result_multiply)
print ("Exponential: ",result_exp)
